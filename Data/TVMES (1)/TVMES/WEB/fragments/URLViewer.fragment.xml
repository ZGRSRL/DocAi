<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="URL Viewer" draggable="true" resizable="true" contentWidth="800px" contentHeight="600px">
        <content>
            <HTML content="<iframe src='{url}' width='100%' height='100%' frameborder='0'></iframe>" sanitizeContent="false" />
        </content>
        <beginButton>
            <Button text="Close" press="onCloseFragment" />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>